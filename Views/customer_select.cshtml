<h1>Cinema Scene</h1>
<hr />
<h2>Select a movie to purchase:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var movie in Model)
    {
      <li><strong>Title:</strong> <a href='/customer/select/@movie.GetId()'>@movie.GetTitle()</a>, <strong>Rating:</strong>@movie.GetRating()</li>
    }
  </ul>
}

<h4><a href="/">Return home</a></h4>
