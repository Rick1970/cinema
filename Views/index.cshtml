<h1>Welcome to the Cinema Scene Home Page!</h1>

<h2>Movies:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var movie in Model)
    {
      <li><strong><a href='/movies/@movie.GetId()'>@movie.GetTitle()</a></strong></li>
    }
  </ul>
}


